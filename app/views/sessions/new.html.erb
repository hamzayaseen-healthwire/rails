<h1>Login</h1>

<%= form_with(url: sessions_path) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit 'Login' %>
  </div>
<% end %>


  Dont have a account! <%= button_to 'Signup',  new_user_path , qamethod: :get %>




